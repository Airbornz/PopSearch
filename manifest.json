{
  "manifest_version": 2,

  "name": "PopSearch",
  "description": "Search popular sites YouTube, and Spotify on the fly!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "menu.html"
  },
  
  "icons": { 
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png" 
  },
  
  "permissions": [
    "tabs"
  ]
}